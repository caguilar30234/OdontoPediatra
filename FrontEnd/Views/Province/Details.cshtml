@model FrontEnd.Models.ProvinceViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAccounts.cshtml";
}

<h1>Provincia</h1>

<div>
    <h4>Detalle Provincia</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProvinceId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProvinceId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProvinceName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProvinceName)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.ProvinceId }, new{@class="btn btn-success"}) |
    <a asp-action="Index" class="btn btn-primary">Regresar a la lista</a>
</div>
